CREATE TABLE "GUEST" (
  "ID" BIGINT NOT NULL PRIMARY KEY GENERATED ALWAYS AS IDENTITY,
  "FULLNAME" VARCHAR(255) NOT NULL
);

CREATE TABLE "ROOM" (
  "ID" BIGINT PRIMARY KEY GENERATED ALWAYS AS IDENTITY,
  "NUMBER" INTEGER,
  "NUMBEROFBEDS" INTEGER,
  "BALCONY" BOOLEAN,
  "PRICE" DECIMAL
);

CREATE TABLE "ACCOMMODATION" (
  "ID" BIGINT PRIMARY KEY GENERATED ALWAYS AS IDENTITY,
  "ARRIVAL" TIMESTAMP,
  "DEPARTURE" TIMESTAMP,
  "ROOM" BIGINT,
  "GUEST" BIGINT
);